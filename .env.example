# Environment variables for NLWeb Crawler
# Copy this file to .env and fill in your values

# OpenAI API Key (required for embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# Google Maps API Key (optional)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Data directory (default: ./data)
DATA_DIR=./data

# External NLWeb project path (adjust as needed)
NLWEB_EXTERNAL_PATH=/home/ivob/Projects/NLWebProjects/dev/NLWeb/code/python

# Development settings
DEVELOPMENT=true
LOG_LEVEL=DEBUG

# Database settings (if using external database)
# QDRANT_URL=http://localhost:6333
# AZURE_SEARCH_ENDPOINT=your_azure_search_endpoint
# AZURE_SEARCH_KEY=your_azure_search_key
